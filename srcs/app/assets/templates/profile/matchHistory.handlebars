<div class="profileBlock" id="matchHistoryBlock">
	<div class="filter matches-filter" id="matches-filter">
		<div class="match-type" id="match-type">
			{{type}}
		</div>
		<div class="arrow-down-container">
			<img src='./icons/arrow_down-black.svg' class="arrow-down"></img>
		</div>
	</div>
	<div class="matches-list" id="matches-list" style="display: none;">
		{{#each match}}
			<div class="el match" id="{{type}}">{{type}}</div>
		{{/each}}
	</div>


	<div class="match-history">
        {{#each myDone}}
        <div class="match-done-container">
            <div class="match-done">
				<div class="nb">{{nb}}</div>
				<div class="center">
					<div class="image-nickname" onclick="window.location='#profile/{{opponentId1}}/';">
						<div class="opponent1">{{opponent1}}</div>
						<div class="image-container">
							<img class="image-url" src="{{avatarOpponent1}}">
						</div>
					</div>
					<div class="score score1">{{score1}}</div>
					<div class="VS">VS</div>
					<div class="score score2">{{score2}}</div>
					<div class="image-nickname" onclick="window.location='#profile/{{opponentId2}}/';">
						<div class="image-container">
							<img class="image-url" src="{{avatarOpponent2}}">
						</div>
						<div class="opponent2">{{opponent2}}</div>
					</div>
				</div>
            </div>
            {{#if forfeit}}
            <div class="forfeit">Victory by forfeit</div>
            {{/if}}
        </div>
		{{/each}}
	</div>

</div>