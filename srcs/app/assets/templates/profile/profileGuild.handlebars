<section class="profileBlock" id="profileGuildBlock">
<div id="guildOptionsContainer">
<span id="guildOptions">
<a id="guildNameLabel" href="#guild/{{this.guild.id}}">{{this.guild.name}}<span> [{{this.guild.anagram}}]</span></a>
</span>
<span id="manageGuildButton"></span>
<span id="leaveGuildButton"></span>
</div>
<section>
<table class="usersList">
<tr class="usersListIndex">
	<th>{{this.membersNumber}} members</th>
	<th></th>
	<th></th>
	<th>Pseudo</th>
	<th>league Rank</th>
	<th>General Rank</th>
	<th>Victories</th>
	<th>Total Games</th>
	<th>Status</th>
	<th>Status</th>
</tr>

<tr class="usersListEl">
	<td>Gold</td>
	<td>1</td>
	<td>profilePic</td>
	<td><a href="#profile/{{owner_id}}/">{{this.owner.nickname}}</a></td>
	<td>1</td>
	<td>1000</td>
	<td>42</td>
	<td>1001</td>
	<td>online</td>
	<td>Owner</td>
</tr>

	{{#each this.officers}}
	<tr class="usersListEl">
			<td>Gold</td>
			<td>1</td>
			<td>profilePic</td>
			<td><a href="#profile/{{id}}/">{{nickname}}</a></td>
			<td>2</td>
			<td>3500</td>
			<td>12</td>
			<td>50</td>
			<td>offline</td>
			<td>Officer</td>
	</tr>
	{{/each}}

	{{#each this.members}}
	<tr class="usersListEl">
			<td>Gold</td>
			<td>1</td>
			<td>profilePic</td>
			<td><a href="#profile/{{id}}/">{{nickname}}</a></td>
			<td>2</td>
			<td>4</td>
			<td>1</td>
			<td>2</td>
			<td>offline</td>
			<td>Member</td>
	</tr>
	{{/each}}
</table>
</section>

</section>>
