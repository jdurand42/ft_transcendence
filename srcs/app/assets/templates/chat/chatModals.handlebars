    <div class="modal modalCreateChannel" id="modalCreateChannel" >
        <div class="background"></div>
        <div class="modalOpen">
            <div class="close closeModal">
                <img src="./icons/close.svg" id="close"></img>
            </div>
            <label class="label labelChannelName" for="channelName">Channel name</label>
            <input class="textInput channelName" type="text" id="channelName" placeholder="New channel"></input>
            <p class="error-message" id="error-message"></p>
            <label class="label labelSelectFriends" for="search">Select friends</label>
            <input class="textInput search modalSearchChannels" type="text" id="modalSearchChannels" placeholder="Research"></input>
            <div class="friends modalSearchChannels" id="friendsmodalSearchChannels">
                {{#each friends}}
                <div class="eachFriend eachFriendModalCreateChannel" for="friend{{id}}">
                    <div class="left">
                        <div class="image-container">
                            <img src={{image_url}} id="blockViewProfile image_url" class="image_url"></img>
                        </div>
                        <p class="anagram_nickname">{{anagram}} {{nickname}}</p>
                    </div>
                    <div class="right">
                        <label class="checkboxlabel" for="{{checkboxId}}"></label>
                        <input type="checkbox" class="checkbox" id="friend{{id}}" value="{{id}}"></input>
                    </div>
                </div>
                {{/each}}
            </div>
            <div class="button-container">
                <button class="createChannel">Create channel</button>
            </div>
        </div>
    </div>
    <div class="modal modalCreateDirectMessages" id="modalCreateDirectMessages">
        <div class="background"></div>
        <div class="modalOpen">
            <div class="close closeModal">
                <img src="./icons/close.svg" id="close"></img>
            </div>
            <label class="label labelSelectFriends" for="search">Select friends</label>
            <input class="textInput search modalSearchDirectMessages" type="text" id="modalSearchDirectMessages" placeholder="Research"></input>
            <div class="friends friendsmodalSearchDirectMessages" id="friendsmodalSearchDirectMessages">
                {{#each friends}}
                <div class="eachFriend eachFriendModalCreateDirectMessages" for="{{id}}">
                    <div class="left">
                        <div class="image-container">
                            <img src={{image_url}} id="image_url" class="image_url"></img>
                        </div>
                        <p class="anagram_nickname">{{anagram}} {{nickname}}</p>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    <div class="modal modalSearchChannel" id="modalSearchAllChannels">
        <div class="background"></div>
        <div class="modalOpen">
            <div class="close closeModal">
                <img src="./icons/close.svg" id="close"></img>
            </div>
            <label class="label labelSelectChannel" for="search">Channels</label>
            <input class="textInput inputModalSearchAllChannels" type="text" id="inputModalSearchAllChannels" placeholder="Research"></input>
            <div class="friends searchAllChannel" id="searchAllChannel">
                {{#each channels}}
                <div class="eachFriend eachChannel" for="{{id}}">
                    {{!-- <div class="left"> --}}
                        {{!-- <div class="image-container">
                            <img src={{image_url}} id="image_url" class="image_url"></img>
                        </div> --}}
                        <p class="channel anagram_nickname">#{{name}}</p>
                    {{!-- </div> --}}
                    {{!-- <div class="right">
                        <label class="checkboxlabel" for="{{checkboxId}}"></label>
                        <input type="checkbox" class="checkbox" id="{{id}}" value="{{id}}"></input>
                    </div> --}}
                </div>
                <div class="modal validationModal modalPassword" id="modalPassword{{id}}" style="display: none;">
                    <div class="background"></div>
                    <div class="modalOpen">
                        <div class="close closeModal">
                            <img src="./icons/close.svg" id="close"></img>
                        </div>
                        <label class="label labelModalPassword" for="labelModalPassword">Password</label>
                        <input class="inputModalPassword" type="password" id="inputModalPassword{{id}}" placeholder="Password" for={{id}}></input>
                        <div class="button-container buttons">
                            <button class="validation-buttons button validate-password" for="{{id}}">Validate</button>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    <div class="modal validationModal" id="modalValidationDeleteChannel">
        <div class="background"></div>
        <div class="modalOpen">
            <p class="sure">Are you sure to want to leave this channel?</p>
            <p class="adminRights">You will lose your administrator rights</p>
            <div class="buttons">
                <div class="validation-buttons button-container-yes">
                    <button class="button yes yesDeleteChannel">Yes</button>
                </div>
                <div class="validation-buttons button-container-no">
                    <button class="button no">No</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal modalAddFriendsToChannel" id="modalAddFriendsToChannel" >
        <div class="background"></div>
        <div class="modalOpen">
            <div class="close closeModal">
                <img src="./icons/close.svg" id="close"></img>
            </div>
            <label class="label labelSelectFriends" for="search">Select friends</label>
            <input class="textInput  modalSearchChannels modalSearchAddFriendsToChannel" type="text" id="modalSearchAddFriendsToChannel" placeholder="Research" for={{id}}></input>
            <div class="friends modalSearchAddFriendsToChannel" id="friendsmodalSearchAddFriendsToChannel">
                {{#each friends}}
                <div class="eachFriend eachFriendModalCreateChannel" for="addfriends{{id}}">
                    <div class="left">
                        <div class="image-container">
                            <img src={{image_url}} id="image_url" class="image_url"></img>
                        </div>
                        <p class="anagram_nickname">{{anagram}} {{nickname}}</p>
                    </div>
                    <div class="right">
                        <label class="checkboxlabel" for="{{checkboxId}}"></label>
                        <input type="checkbox" class="checkbox" id="addfriends{{id}}" value="{{id}}"></input>
                    </div>
                </div>
                {{/each}}
            </div>
            <div class="button-container buttons">
                <button class="validation-buttons button validate-add-friends" for="{{chatId}}">Validate</button>
            </div>
        </div>
    </div>
    <div class="parameters" id="params">
        <div class="left-menu">
            <div class="menu">
                <div class="sub-menu overview-menu" id="overview-menu"><p class="p">Overview</p></div>
                {{#if owner}}
                <div class="sub-menu permissions-menu"  id="permissions-menu"><p class="p">Permissions</p></div>
                {{/if}}
                <div class="sub-menu members-menu" id="members-menu"><p class="p">Members</p></div>
                {{#ifCond owner '||' superAdmin}}
                <div class="deleteChannel" >Delete channel</div>
                {{/ifCond}}
            </div>
        </div>
        <div class="modal validationModal modalValidationDeleteDefinitivelyChannel" id="modalValidationDeleteDefinitivelyChannel" style="display: none;">
            <div class="background"></div>
            <div class="modalOpen">
                <p class="sure">Are you sure to want to delete this channel?</p>
                <p class="adminRights">Any members won't have acces to this channel anymore</p>
                <div class="buttons">
                    <div class="validation-buttons button-container-yes">
                        <button class="button yes yesDeleteDefinitivelyChannel">Yes</button>
                    </div>
                    <div class="validation-buttons button-container-no">
                        <button class="button no">No</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="right-menu">
            <div class="close closeParams">
                <img src="./icons/esc.svg" class="">
            </div>
            <div class="page overview" id="params-overview">
                <div class="title">OVERVIEW</div>
                <div class="sub-title">CHANNEL NAME</div>
                <input type="text" class="name" id="inputName" disabled value={{name}}></input>
            </div>
            <div class="page permissions" id="params-permissions">
                <div class="title">PERMISSIONS</div>
                <div class="sub-title">CONFIDENTIALITY</div>
                <div class="radio-button">
                    <input type="radio" class="private" id="private" name="privacy" value="private" checked>
                    <label for="private">Private</label> 
                </div>
                <div class="radio-button">
                    <input type="radio" class="public" id="public" name="privacy" value="public">
                    <label for="public">Public</label> 
                </div>
                <div class="radio-button">
                    <input type="radio" class="protected"  id="protected" name="privacy" value="protected"></input>
                    <label for="protected">Protected</label> 
                </div>
                <div class="password" id="passwordDiv" style="display: none;">
                    <label for="password">Password</label>
                    <input type="password" id="password" name="password"></input>
                    <div class="passwordVisibility" id="visibility">
                        <img src='./icons/visibility_off.svg' class="icon-visibility " id="eyeVisibility"></img>
                    </div>
                </div>
                <p class="error-message error-password" id="error-password" style="display: none;"></p>
                <button class="save">Save</button>
            </div>
            <div class="page members" id="params-members">
                <div class="title">MEMBERS</div>
                <div class="sub-title">OWNER</div>
                <div class="list owner">
                    {{#each owners}}
                    <div class="self owner">
                        <div class="image-container">
                            <img src={{image_url}} class="image_url" id={{channelId}} for="{{id}}"></img>
                        </div>
                        <div class="anagram">
                            <p class="text">{{anagram}}</p>
                        </div>
                        <div class="nickname">
                            <p class="text">{{nickname}}</p>
                        </div>
                    </div>
                    {{/each}}
                </div>
                <div class="sub-title">ADMINISTRATORS</div>
                <div class="list admins" id="admins">
                    {{#each admins}}
                        <div class="self admin">
                            <div class="image-container">
                                <img src={{image_url}} class="image_url" id={{channelId}} for="{{id}}"></img>
                            </div>
                            <div class="anagram">
                                <p class="text">{{anagram}}</p>
                            </div>
                            <div class="nickname">
                                <p class="text">{{nickname}}</p>
                            </div>
                            {{#if superAdmin}}
                            <div class="dots-container" for="{{id}}">
                                <img src='./icons/dots.svg' class="dots"></img>
                            </div>
                            <div class="admin-rights" id="admin-rights{{id}}" style="display: none;">
                                <div class="el remove-admin-rights" id="removeAdminRights" for="{{id}}">Remove administrator rights</div>
                            </div>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
                <div class="sub-title">OTHER MEMBERS</div>
                <div class="list participants" id="participants">
                    {{#each members}}
                        <div class="self member">
                            <div class="image-container">
                                <img src={{image_url}} class="image_url" id={{channelId}} for="{{id}}"></img>
                            </div>
                            <div class="anagram">
                                <p class="text">{{anagram}}</p>
                            </div>
                            <div class="nickname">
                                <p class="text">{{nickname}}</p>
                            </div>
                            <div class="dots-container" for="{{id}}">
                                <img src='./icons/dots.svg' class="dots"></img>
                            </div>
                            <div class="admin-rights" id="admin-rights{{id}}" style="display: none;">
                                {{#ifCond owner '||' superAdmin}}
                                <div class="el appoint-as-admin" id="appointAsAdministrator" for="{{id}}">Appoint as administrator</div>
                                {{/ifCond}}
                                <div class="el mute" id="mute" for="{{id}}">Mute</div>
                                <div class="el ban" id="ban" for="{{id}}">Ban</div>
                            </div>
                        </div>
                        <div class="modal validationModal modalValidationAppointAsAdmin" id="modalValidationAppointAsAdmin{{id}}" style="display: none;">
                            <div class="background"></div>
                            <div class="modalOpen">
                                <p class="sure">Appoint this member as administrator</p>
                                <p class="adminRights">{{nickname}} will be able to mute and ban other members.</p>
                                <div class="buttons">
                                    <div class="validation-buttons button-container-yes">
                                        <button class="button yes yesAsAdmin" for="{{id}}">Yes</button>
                                    </div>
                                    <div class="validation-buttons button-container-no">
                                        <button class="button no">No</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal validationModal modalValidationBan" id="modalValidationBan{{id}}" style="display: none;">
                            <div class="background"></div>
                            <div class="modalOpen">
                                <p class="sure">Ban this member</p>
                                <p class="adminRights">{{nickname}} won't be able to find, see or join this channel.</p>
                                <div class="radio-buttons" >
                                    <div class="radio-button">
                                        <input type="radio" id="1minBan" name="radioBan{{id}}" value="60" checked>
                                        <label for="1minBan">1 minute</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="10minBan" name="radioBan{{id}}" value="600">
                                        <label for="10minBan">10 minutes</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="12hBan" name="radioBan{{id}}" value="43200">
                                        <label for="12hBan">12 hours</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="24hBan" name="radioBan{{id}}" value="86400">
                                        <label for="24hBan">24 hours</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="7daysBan" name="radioBan{{id}}" value="604800">
                                        <label for="7daysBan">7 days</label> 
                                    </div>
                                </div>
                                <div class="buttons">
                                    <div class="validation-buttons button-container-yes">
                                        <button class="button yes yesBan" for="{{id}}">Ban</button>
                                    </div>
                                    <div class="validation-buttons button-container-no">
                                        <button class="button no">Cancel</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal validationModal modalValidationMute" id="modalValidationMute{{id}}" style="display: none;">
                            <div class="background"></div>
                            <div class="modalOpen">
                                <p class="sure">Mute this member</p>
                                <p class="adminRights">{{nickname}} will have access to this channel, but couldn't send messages.</p>
                                <div class="radio-buttons" >
                                    <div class="radio-button">
                                        <input type="radio" id="1minBan" name="radioMute{{id}}" value="60" checked>
                                        <label for="1minMute">1 minute</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="10minBan" name="radioMute{{id}}" value="600">
                                        <label for="10minMute">10 minutes</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="12hBan" name="radioMute{{id}}" value="43200">
                                        <label for="12hMute">12 hours</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="24hBan" name="radioMute{{id}}" value="86400">
                                        <label for="24hMute">24 hours</label> 
                                    </div>
                                    <div class="radio-button">
                                        <input type="radio" id="7daysBan" name="radioMute{{id}}" value="604800">
                                        <label for="7daysMute">7 days</label> 
                                    </div>
                                </div>
                                <div class="buttons">
                                    <div class="validation-buttons button-container-yes">
                                        <button class="button yes yesMute" for="{{id}}">Mute</button>
                                    </div>
                                    <div class="validation-buttons button-container-no">
                                        <button class="button no">Cancel</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>